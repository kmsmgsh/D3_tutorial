{
  "name": "d3_tutorial",
  "version": "1.0.0",
  "type": "module",
  "main": "script.js",
  "scripts": {
    "dev": "vite --port 4173",
    "build": "vite build && npm run copy-assets",
    "copy-assets": "cp -r docs dist/ && cp -r sample-data dist/",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "docs:api": "jsdoc script.js -d docs/",
    "docs:api:serve": "jsdoc script.js -d docs/ && open docs/index.html",
    "docs:tutorial": "vitepress dev tutorial",
    "docs:tutorial:build": "vitepress build tutorial",
    "docs:tutorial:preview": "vitepress preview tutorial",
    "docs:all": "npm run docs:api && npm run docs:tutorial:build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "gh-pages": "^6.3.0",
    "jsdoc": "^4.0.4",
    "vite": "^7.1.3",
    "vitepress": "^1.6.4"
  },
  "dependencies": {
    "d3": "^7.9.0",
    "monaco-editor": "^0.52.2",
    "vega-embed": "^6.25.0",
    "vega-lite": "^5.20.1"
  }
}
